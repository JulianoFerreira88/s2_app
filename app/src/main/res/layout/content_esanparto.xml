<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <androidx.cardview.widget.CardView
                style="@style/card"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Matriz"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView6" android:layout_weight="1"
                            android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtPartoMatriz"
                            android:layout_weight="1" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Data Início"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView10"
                            android:layout_weight="1" android:gravity="left"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtDtInicio"
                            android:layout_weight="1" android:layout_gravity="right"
                            android:layout_marginRight="5dp" android:layout_marginEnd="5dp" android:gravity="left"/>
                    <TextView
                            android:text="Data Fim"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView11"
                            android:layout_weight="1" android:gravity="left"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtDtFim"
                            android:layout_weight="1" android:layout_gravity="right" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Hora Início"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView12"
                            android:layout_weight="1" android:gravity="left"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtHoraInicio"
                            android:layout_weight="1" android:layout_gravity="right"
                            android:layout_marginRight="5dp" android:layout_marginEnd="5dp" android:gravity="left"/>
                    <TextView
                            android:text="Hora Fim"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView13"
                            android:layout_weight="1" android:gravity="left"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView24HoraFim"
                            android:layout_weight="1" android:layout_gravity="right" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Duração"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView14"
                            android:layout_weight="1" android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtDuracao"
                            android:layout_weight="1" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Tipo de Parto"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView16"
                            android:layout_weight="1" android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtTipoParto"
                            android:layout_weight="1" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout"
                >
                    <TextView
                            android:text="Funcionário"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/textView19"
                            android:layout_weight="1" android:layout_marginRight="5dp" android:layout_marginEnd="5dp"/>
                    <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content" android:id="@+id/txtFuncionario"
                            android:layout_weight="1" android:gravity="right"/>
                </LinearLayout>
                <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content" android:layout_margin="@dimen/mg_linear_layout">
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="NV"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView33"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtNV"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="NM"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView31"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtNM"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="MUM"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView29"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtMUM"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="MN"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView37"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtMN"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="BV"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView35"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtBV"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="Peso Médio Leitg."
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView41"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtPesoMedio"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent" android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp">
                        <TextView
                                android:text="Peso Leitg."
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/textView38"
                                android:gravity="left" android:layout_weight="1"/>
                        <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content" android:id="@+id/txtPesoTotal"
                                android:gravity="right" android:layout_weight="1"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </LinearLayout>
</RelativeLayout>